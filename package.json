{
	"name": "resharper9-keybindings",
	"displayName": "Resharper 9 Keybindings",
	"version": "0.0.1",
	"private": false,
	"description": "Port of Resharper 9 Keybindings",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandResharperKeybindings#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandResharperKeybindings/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandResharperKeybindings.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"keybindings": [
			{
				"command": "editor.action.copyLinesDownAction",
				"key": "ctrl+d",
				"mac": "cmd+d",
				"when": "editorTextFocus"
			},
			{
				"command": "editor.action.deleteLines",
				"key": "ctrl+l",
				"mac": "cmd+l",
				"when": "editorTextFocus"
			},
			{
				"command": "workbench.action.files.saveAll",
				"key": "ctrl+shift+s",
				"mac": "cmd+shift+s"
			},
			{
				"command": "workbench.action.files.revealActiveFileInWindows",
				"key": "ctrl+alt+l",
				"mac": "cmd+alt+l"
			},
			{
				"command": "workbench.action.quickOpen",
				"key": "ctrl+n",
				"mac": "cmd+n"
			},
			{
				"command": "editor.action.goToTypeDeclaration",
				"key": "ctrl+m",
				"mac": "cmd+m"
			},
			{
				"command": "workbench.action.navigateBack",
				"key": "ctrl+-",
				"mac": "cmd+-"
			},
			{
				"command": "workbench.action.gotoSymbol",
				"key": "ctrl+shift+alt+n",
				"mac": "ctrl+shift+alt+n"
			},
			{
				"command": "editor.action.smartSelect.grow",
				"key": "ctrl+w",
				"mac": "cmd+w",
				"when": "editorTextFocus"
			},
			{
				"command": "editor.action.smartSelect.shrink",
				"key": "ctrl+shift+w",
				"mac": "cmd+shift+w",
				"when": "editorTextFocus"
			},
			{
				"command": "editor.action.commentLine",
				"key": "ctrl+7",
				"mac": "cmd+7",
				"when": "editorTextFocus"
			}
		]
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6"
	},
	"publishConfig": {
		"access": "public"
	},
	"icon": "keyboard.png",
	"galleryBanner": {
		"color": "#0000FF",
		"theme": "dark"
	}
}
